pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                class Example {
  	        	    static void main(String[] args) {
      				// Using a simple println statement to print output to the console
				        println('Hello World');
   				}
		}
            }
        }
    }
}
